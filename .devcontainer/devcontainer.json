{
  "name": "Rust-Alpine",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "settings": {
    "workbench.colorTheme": "One Dark Pro",
    "locale": "zh-cn"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "MS-CEINTL.vscode-language-pack-zh-hans", // 简体中文语言包
        "zhuangtongfa.Material-theme",           // One Dark Pro主题
        "rust-lang.rust-analyzer"                // Rust语言支持
      ]
    }
  },
  "remoteUser": "vscode"
}
